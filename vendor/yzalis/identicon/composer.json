{
    "name": "yzalis/identicon",
    "type": "library",
    "description": "Create awesome unique avatar.",
    "keywords": [
        "avatar",
        "identicon",
        "image"
    ],
    "homepage": "http://identicon-php.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Laugueux",
            "email": "benjamin@yzalis.com"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "ext-imagick": "*",
        "fzaninotto/faker": "^1.2.0",
        "phpunit/phpunit": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Identicon\\": "src/Identicon/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Identicon\\Tests\\": "tests/Identicon/Tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
