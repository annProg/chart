{
    "name": "zyan/license-plate-number",
    "keywords": ["车牌生成器"],
    "description": "车牌生成器",
    "license": "MIT",
    "authors": [
        {
            "name": "读心印",
            "email": "aa24615@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "~6.0|~7.0",
        "ext-json": "*",
        "intervention/image": "^2.7",
      "ext-gd": "*"
    },
    "autoload": {
        "psr-4": {
            "Zyan\\LicensePlateNumber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^8.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0"
    },
    "extra": {

    },
    "scripts": {
        "fix": "php-cs-fixer fix --using-cache=no --ansi",
        "test": "vendor/bin/phpunit --colors=always"
    },
    "scripts-descriptions": {
        "test": "Run all tests.",
        "fix": "Run style checks and fix violations."
    }
}
